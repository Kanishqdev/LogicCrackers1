<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.calculator.kanis.logiccrackers">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/algorithm"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EasyLevel" />
        <activity android:name=".HardLevel" />
        <activity android:name=".EasyGame" />
        <activity android:name=".finalScore" />
        <activity android:name=".hardgame" />
        <activity android:name=".Easy_sets" />
        <activity android:name=".hardsets"></activity>
    </application>

</manifest>